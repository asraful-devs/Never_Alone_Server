model Event {
  id          String   @id @default(uuid())
  title       String
  description String
  eventImage  String?
  date        DateTime
  fee         Float
  seats       Int
  location    String

  host Host @relation(fields: [hostId], references: [id])

  hostId String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
